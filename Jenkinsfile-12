pipeline {
    agent  { label 'agent-01' }


    stages {
        
        stage('compile') {
            steps {
               sh 'mvn compile' 
            }
        }
        stage('test') {
            steps {
                 sh 'mvn test' 
            }
        }
        stage('package') {
            steps {
                 sh 'mvn package' 
            }
        }
        stage('Archive') {
            steps {

                archiveArtifacts artifacts: 'target/*.jar', followSymlinks: false, onlyIfSuccessful: true
            }
        }

       
    }
}


